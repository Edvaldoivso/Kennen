var Wverifica = $("*").hasClass("border-danger")

if (Wverifica == true) {
    _cc.msg("Campos com Problemas verifique os Campos em vermelho ", "danger")
} else {
    /* Remove os Relatorios Já existentes*/
    $("#modelo").remove()
    /* Pega os Valores dos Meus campos*/
